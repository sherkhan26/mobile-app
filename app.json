{
	"expo": {
		"name": "my-mobile-app",
		"slug": "my-mobile-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./app/assets/icon.png",
		"userInterfaceStyle": "light",
		"jsEngine": "hermes",
		"newArchEnabled": true,
		"splash": {
			"image": "./app/assets/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"requireFullScreen": true,
			"bundleIdentifier": "com.maxzbs.redflix",
			"jsEngine": "hermes"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./app/assets/adaptive-icon.png",
				"backgroundColor": "#ffffff",
				"jsEngine": "hermes"
			},
			"edgeToEdgeEnabled": true,
			"predictiveBackGestureEnabled": false
		},
		"web": {
			"favicon": "./app/assets/favicon.png"
		},
		"extra": {
			"eas": {
				"projectId": "6b58d1cf-c30d-4ab1-9f27-f19a05eb3fb5"
			}
		}
	}
}
